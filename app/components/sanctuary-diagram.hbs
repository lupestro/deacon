<svg class="sanctuary" width={{@width}} height={{@height}}>
  {{#each @pews as |pew| }}
    <PewFigure @model={{pew}} />
  {{/each}}
  {{#each @saints as |saint|}}
    <SaintFigure @model={{saint}} />
  {{/each}}
  {{#each @deacons as |deacon| }}
    <DeaconFigure @model={{deacon}} />
  {{/each}}
  {{#each @plates as |plate| }}
    <PlateFigure @model={{plate}} />
  {{/each}}
</svg>